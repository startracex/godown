{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/*.js"
      ]
    },
    "check-types": {},
    "clean": {},
    "lint": {},
    "fmt": {},
    "godown#dev": {
      "dependsOn": [
        "@godown/f7-icon#build",
        "@godown/colors#build",
        "@godown/element#build"
      ]
    }
  }
}
