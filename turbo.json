{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "**/*.js",
        "**/*.d.ts",
        "**/*.map"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "clean": {},
    "lint": {},
    "fmt": {},
    "godown#dev": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
