{
  "name": "rollup-plugin-cjs-shim",
  "version": "1.0.0",
  "description": "Rollup plugin to shim CJS modules",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "require": "./index.cjs",
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {
    "typescript": "^5.8.3"
  },
  "keywords": [
    "cjs",
    "shim",
    "rollup",
    "plugin",
    "replace"
  ],
  "author": "startracex",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/godown.git",
    "directory": "plugins/rollup-cjs-shim"
  },
  "files": [
    "index.js",
    "index.cjs",
    "index.d.ts"
  ]
}
