{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "useDefineForClassFields": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "outDir": "out",
    "jsx": "react"
  },
  "include": [
    ".storybook/*",
    "."
  ]
}
