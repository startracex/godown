{
  "name": "godown",
  "version": "3.5.3",
  "description": "Non Composite Web Components Library",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "clean": "tsc --build tsconfig.prod.json --clean",
    "build": "tsx build.ts",
    "build:ts": "tsc",
    "fmt": "prettier src --write",
    "lint": "biome lint",
    "manifest": "tsx manifest.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/startracex/godown.git",
    "directory": "packages/godown"
  },
  "keywords": [
    "web-components",
    "custom-elements"
  ],
  "author": "startracex",
  "license": "MIT",
  "dependencies": {
    "@godown/colors": "workspace:^",
    "@godown/element": "workspace:^",
    "@godown/f7-icon": "workspace:^",
    "fmtime": "^0.0.3",
    "lit": "catalog:",
    "tslib": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "**/*.{js,d.ts}+(|.map)",
    "custom-elements.json",
    "web-types.json",
    "vscode.{css,html}-custom-data.json",
    "src",
    "tsconfig.json"
  ],
  "custom-elements": "./custom-elements.json",
  "customElements": "./custom-elements.json",
  "web-types": "./web-types.json"
}
