{
  "name": "godown",
  "version": "3.0.0-canary.10",
  "description": "Non Composite Web Components Library",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "development": "./dev/index.js",
      "default": "./index.js"
    },
    "./*": {
      "development": "./dev/*",
      "default": "./*"
    },
    "./*.json": "./*.json"
  },
  "scripts": {
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit",
    "clean": "tsc --build tsconfig.prod.json --clean",
    "build": "tsc && tsx build.ts",
    "build:ts": "tsc",
    "fmt": "dprint fmt",
    "lint": "eslint . --fix",
    "manifest": "tsx manifest.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/startracex/godown.git",
    "directory": "packages/godown"
  },
  "keywords": [],
  "author": "startracex",
  "license": "MIT",
  "dependencies": {
    "@godown/colors": "workspace:^",
    "@godown/element": "workspace:^",
    "@godown/f7-icon": "workspace:^",
    "fmtime": "^0.0.3",
    "lit": "^3.0.0",
    "tslib": "^2.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "**/*.{js,d.ts}+(|.map)",
    "custom-elements.json",
    "web-types.json",
    "vscode.{css,html}-custom-data.json",
    "src"
  ],
  "custom-elements": "./custom-elements.json",
  "customElements": "./custom-elements.json",
  "web-types": "./web-types.json"
}