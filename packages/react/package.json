{
  "name": "@godown/react",
  "version": "3.13.3",
  "description": "React components for godown",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "tsc --build --clean",
    "build": "tsc"
  },
  "devDependencies": {
    "react": "^19.1.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@lit/react": "catalog:",
    "godown": "workspace:^"
  },
  "exports": {
    ".": "./build/index.js",
    "./package.json": "./package.json",
    "./src/*": "./src/*",
    "./*.js": "./build/*.js",
    "./*": "./build/*.js"
  },
  "files": [
    "**/*.{js,d.ts}+(|.map)",
    "src"
  ],
  "keywords": [
    "web-components",
    "custom-elements"
  ],
  "homepage": "https://godown.js.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/godown.git",
    "directory": "packages/react"
  },
  "author": "startracex",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
