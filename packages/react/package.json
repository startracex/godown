{
  "name": "@godown/react",
  "version": "3.7.5",
  "description": "React components for godown",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json",
    "./*.js": "./*.js",
    "./*": "./*.js"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "tsc --build --clean",
    "build": "tsc",
    "fmt": "dprint fmt",
    "lint": "biome lint"
  },
  "homepage": "https://godown.js.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/startracex/godown.git",
    "directory": "packages/react"
  },
  "keywords": [
    "web-components",
    "custom-elements"
  ],
  "author": "startracex",
  "license": "MIT",
  "dependencies": {
    "@lit/react": "catalog:",
    "godown": "workspace:^",
    "react": "*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "**/*.{js,d.ts}+(|.map)",
    "src"
  ]
}
