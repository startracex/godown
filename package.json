{
  "name": "@godown/workspace",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "build:docs": "pnpm run --filter @godown/storybook storybook-build",
    "build:main": "turbo run build --filter godown",
    "build:plugins": "turbo run build --filter ./plugins/*",
    "check": "turbo run lint fmt",
    "check-types": "turbo run check-types",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "fmt": "turbo run fmt",
    "lint": "turbo run lint",
    "version": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@godown/tsconfig": "workspace:*",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cem-plugin-define": "workspace:^",
    "cem-plugin-fix-module": "workspace:^",
    "dprint": "^0.47.6",
    "eslint": "^8.57.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "glob": "^11.0.0",
    "postcss": "^8.4.49",
    "rollup": "catalog:",
    "rollup-plugin-cjs-shim": "workspace:^",
    "rollup-plugin-template-replace": "workspace:^",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "typescript545": "npm:typescript@5.4.5",
    "vite": "^6.0.3"
  },
  "author": "startracex",
  "license": "MIT",
  "description": "Simple, stylized, semantic web components",
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.15.0"
}
