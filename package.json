{
  "name": "godown",
  "version": "2.0.5",
  "main": "./index.js",
  "type": "module",
  "private": true,
  "publishConfig": {
    "access": "public",
    "directory": "public"
  },
  "scripts": {
    "dev": "node ./scripts/dev.js",
    "watch": "tsc --watch",
    "build": "tsc && node ./scripts/pub.js",
    "roll": "rollup -c",
    "fmt": "prettier --write ./src",
    "lint": "eslint --ext .ts ./src"
  },
  "keywords": [
    "godown",
    "components",
    "lit",
    "html",
    "react",
    "custom elements"
  ],
  "author": "startracex",
  "license": "MIT",
  "description": "simple,stylized,semantic components",
  "repository": {
    "type": "git",
    "url": "https://github.com/startracex/godown.git"
  },
  "dependencies": {
    "@lit/react": "^1.0.0",
    "lit": "^3.0.0",
    "react": "latest"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@web/dev-server": "^0.1.35",
    "@web/rollup-plugin-html": "^1.11.0",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "rollup": "^3.29.4",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
